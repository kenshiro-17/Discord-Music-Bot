# Discord Configuration (Required)
DISCORD_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here



# Sentry Configuration (Optional - for error tracking)
SENTRY_DSN=your_sentry_dsn

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
DEFAULT_VOLUME=50
MAX_QUEUE_SIZE=100
INACTIVITY_TIMEOUT=300
MAX_FILE_SIZE=25

# YouTube Authentication (Highly Recommended to fix 403 errors)
# Export cookies from your YouTube account using a browser extension like "Get cookies.txt LOCALLY"
# Format: Netscape format (tab-separated) or JSON array
# Example: Copy the entire cookies.txt file content here as a single line (escape newlines with \n)
YOUTUBE_COOKIES=
